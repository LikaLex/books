<%= form_for @publisher, url: publishers_path, html: { multipart: true }  do |form| %>
    <%= form.file_field :publisher_avatar %>
    <%= form.input :title%>
    <%= form.input :foundation_date %>
    <%= form.input :country %>
    <%= form.input :founder %>
    <%= form.input :ceo %>
    <%= form.input :description%>
    <%= form.submit %>
<% end %>